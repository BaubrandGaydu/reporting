<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>Connexion</servlet-name>
    <servlet-class>docapost.reporting.controllers.ConnexionServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Accueil</servlet-name>
    <servlet-class>docapost.reporting.controllers.AccueilServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>DetailGraphique</servlet-name>
    <servlet-class>docapost.reporting.controllers.DetailGraphiqueServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Connexion</servlet-name>
    <url-pattern>/connexion</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Accueil</servlet-name>
    <url-pattern>/accueil</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>DetailGraphique</servlet-name>
    <url-pattern>/detailgraphique</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>connexion</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>DetailGraphiqueServlet</display-name>
    <servlet-name>DetailGraphiqueServlet</servlet-name>
    <servlet-class>docapost.reporting.controllers.DetailGraphiqueServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DetailGraphiqueServlet</servlet-name>
    <url-pattern>/DetailGraphiqueServlet</url-pattern>
  </servlet-mapping>
</web-app>